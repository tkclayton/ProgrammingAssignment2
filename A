makeCacheMatrix <- function(x = matrix()) {
	xInv <- NULL
	set <- function(y) {
		x <<- y
		xInv <<- NULL
	}
	get <- function() x
	setInv <- function(inverse) xInv <<- inverse
	getInv <- function() xInv
	list(set = set, get = get, setInv = setInv, getInv = getInv)
}

cacheSolve <- function(x, ...) {
	xInv <- x$getInverse()
	if(!is.null(xInv)) {
		message("geting cached data")
		return(xInv)
	}
	data <- x$get()
	xInv <- Inverse(data, ...)
	x$setInverse(xInv)
	xInv
}
